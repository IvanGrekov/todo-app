{
    "name": "todo-app",
    "version": "0.1.0",
    "scripts": {
        "start:server": "cd ./src/models/server && tsc && node ./dist/index.js",
        "start:app": "PORT=4000 react-scripts start",
        "start": "concurrently --kill-others \"npm run start:server\" \"npm run start:app\"",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "format": "prettier -w . --ignore-path .gitignore",
        "lint": "eslint . --ignore-path .gitignore",
        "lint:fix": "eslint . --ignore-path .gitignore --fix",
        "unused-exports": "ts-unused-exports ./tsconfig.json",
        "fix-code-style": "npm run format && npm run lint",
        "prepare": "husky install"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.1",
        "axios": "^1.2.2",
        "classnames": "^2.3.2",
        "cors": "^2.8.5",
        "focus-trap-react": "^10.0.2",
        "formik": "^2.2.9",
        "localforage": "^1.10.0",
        "match-sorter": "^6.3.1",
        "moment": "^2.29.4",
        "pg": "^8.11.0",
        "pg-hstore": "^2.3.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.5.0",
        "react-scripts": "5.0.1",
        "sass": "^1.57.0",
        "sequelize": "^6.32.0",
        "sort-by": "^1.2.0",
        "styled-components": "^5.3.6",
        "web-vitals": "^2.1.4",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/node": "^18.11.17",
        "@types/pg": "^8.10.2",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@types/styled-components": "^5.1.26",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "concurrently": "^7.6.0",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.2",
        "lint-staged": "^13.1.0",
        "prettier": "^2.8.1",
        "ts-node": "^10.9.1",
        "ts-unused-exports": "^8.0.5",
        "typescript": "^4.9.4"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "lint-staged": {
        "*.{js,ts,jsx,tsx}": "npm run fix-code-style"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
